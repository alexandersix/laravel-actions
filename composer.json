{
    "name": "kirschbaum-development/actions",
    "description": "A test package for intercepting email sent from Laravel",
    "keywords": [
        "laravel",
        "actions",
        "events"
    ],
    "homepage": "https://github.com/kirschbaum-development/actions",
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Ferens",
            "email": "brandon@kirschbaumdevelopment.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "illuminate/events": ">=6.0"
    },
    "require-dev": {
		"codedungeon/phpunit-result-printer": "^0.30.1",
        "friendsofphp/php-cs-fixer": "^3.3.2|^3.6",
        "orchestra/testbench": "^4.18|^5.20|^6.24|^7.0",
        "phpunit/phpunit": "^8.5.8|^9.3.3|^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Kirschbaum\\Actions\\": "src/"
        },
		"files": [
			"src/helpers.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
	"extra": {
		"laravel": {
			"providers": [
				"Kirschbaum\\Actions\\ActionsServiceProvider"
			],
			"aliases": {
				"Action": "Kirschbaum\\Actions\\Facades\\Action"
			}
		}
	}
}
